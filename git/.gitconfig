[init]
  defaultBranch = main

[user]
  email = iseddon@ebsco.com
  name = Ian Seddon

# delta (https://github.com/dandavison/delta)

[core]
  pager = delta

[interactive]
  diffFilter = delta --color-only

[delta]
  navigate = true  # use n and N to move between diff sections
  dark = true

[merge]
  conflictstyle = zdiff3
